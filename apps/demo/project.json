{
  "name": "demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/demo/src",
  "targets": {
    "build": {
      "executor": "qwik-nx:build",
      "options": {
        "runSequence": ["demo:build.client", "demo:build.ssr"],
        "outputPath": "dist/apps/demo"
      },
      "configurations": {
        "preview": {}
      }
    },
    "build.client": {
      "executor": "@nx/vite:build",
      "options": {
        "outputPath": "dist/apps/demo",
        "configFile": "apps/demo/vite.config.ts"
      }
    },
    "build.ssr": {
      "executor": "@nx/vite:build",
      "defaultConfiguration": "preview",
      "options": {
        "outputPath": "dist/apps/demo"
      },
      "configurations": {
        "preview": {
          "ssr": "src/entry.preview.tsx",
          "mode": "production"
        },
        "production": {
          "configFile": "apps/demo/adapters/vercel-edge/vite.config.ts"
        }
      },
      "dependsOn": []
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "demo:build",
        "port": 4300
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["../../coverage/apps/demo"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/demo"
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "demo:build.client",
        "mode": "ssr",
        "port": 4200
      }
    },
    "serve.debug": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node --inspect-brk ../../node_modules/vite/bin/vite.js --mode ssr --force",
        "cwd": "apps/demo"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/demo/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build -c adapters/vercel-edge/vite.config.ts"
      },
      "dependsOn": ["build-vercel"]
    },
    "build-vercel": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build -c adapters/vercel-edge/vite.config.ts"
      }
    }
  },
  "tags": []
}
