{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/docs/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vuepress dev",
        "cwd": "apps/docs"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vuepress build",
        "cwd": "apps/docs"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vuepress serve",
        "cwd": "apps/docs"
      }
    }
  },
  "tags": []
}
